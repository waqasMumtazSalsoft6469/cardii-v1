app_identifier(ENV['BUNDLE_IDENTIFIER']) # The bundle identifier of your app
apple_id(ENV['APPLE_ID']) # Your Apple email address

itc_team_id(ENV['APPLE_STORE_TEAM_ID']) # App Store Connect Team ID
team_id(ENV['DEVEOPER_PORTAL_TEAM_ID']) # Developer Portal Team ID

